{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": ["es2018", "dom"],
    "typeRoots": ["node_modules/@types"],
    "paths": {
      "@shared-material": ["src/app/shared/modules/material.module"],
      "@shared/*": ["src/app/shared/*"],
      "@store/*": ["src/app/store/*"],
      "@my-applications/*": ["src/app/my-applications/*"],
      "@profiles/*": ["src/app/profiles/*"],
      "@environments/*": ["src/environments/*"],
      "@app/*": ["src/app/*"]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
